{
    "manifest_version": 3,
    "name": "Chrome AI Experiment",
    "description": "Access Chrome AI API from side panel.",
    "version": "1.0",
    "action": {
      "default_icon": "images/icons8-globe-50.png"
    },
    "side_panel": {
      "default_path": "sidepanel/sidepanel.html"
    },
    "permissions": [
      "tabs",
      "sidePanel",
      "clipboard-write"
    ]
}